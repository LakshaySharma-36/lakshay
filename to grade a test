#include <stdio.h>
int main() {
    int marks[5], sum = 0, i;
    float average;
    printf("Enter marks for 5 subjects: ");
    for (i = 0; i < 5; i++) {
        scanf("%d", &marks[i]);
        sum += marks[i];
    }
    average = sum / 5.0;
    if (average >= 90) printf("Grade: A+\n");
    else if (average >= 80) printf("Grade: A\n");
    else if (average >= 70) printf("Grade: B+\n");
    else if (average >= 60) printf("Grade: B\n");
    else if (average >= 50) printf("Grade: C\n");
    else printf("Grade: Fail\n");
    return 0;
}
